.title{
  height: 118rpx;
  border-bottom:1rpx solid grey;
  text-align: center;
  line-height:118rpx;
}
.container{
  font-size:32rpx;
}
#client{
  margin:30rpx 40rpx;
  height: 500rpx;
}
.input{
  border:3rpx solid  rgba(0, 0, 0, 0.2)

}
#cname{
  display: flex;
  flex-direction: row;

}
#ctel{
  display: flex;
  margin-top: 52rpx;
  margin-bottom: 28rpx;
}
.picker{
    background-color: #FFFFFF;
    margin: 50rpx 0 40rpx 0;
}
.receiver{
  width: 100%;
  height: 500rpx;
  border-top:3rpx solid  rgba(0, 0, 0, 0.2);

}

#detail{
  margin:10rpx 45rpx 5rpx 0 ;
  height: 120rpx;
}

