/* pages/community/settled/second_settled/second_settled.wxss */
.subword{
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  padding-left: 20rpx;
  font-size: 30rpx;
}
.subphoto{
  width: 706rpx;
  height: 256rpx;
  border: 2rpx solid #dddddd;
  margin: 0rpx auto;
  border-radius: 8rpx;
}
.rightphoto{
  width: 136rpx;
  height: 176rpx;
  margin: 28rpx auto;
  border: 2rpx solid #dddddd;
  padding: 10rpx;
}
.rightphotoiner{
  width: 132rpx;
  height: 146rpx;
  border: 2rpx dashed #dddddd;
  display: block;
}
.upload{
  width: 100%;
  height: 38rpx;
  line-height: 38rpx;
  text-align: center;
  font-size: 24rpx;
}
.botsub{
  margin-bottom: 20rpx;
}
.subbtn{
  width: 710rpx;
  height: 90rpx;
  border-radius: 12rpx;
  padding: 0rpx;
  margin: 40rpx 20rpx 20rpx 20rpx;
  background-color: #577eff;
  color: #fff;
  text-align: center;
  line-height: 90rpx;
  font-size: 30rpx;
}
.addbind{
  width: 132rpx;
  height: 146rpx;
  display: block;
}
.oimg{
  display: block;
  margin: auto;
}
.shade{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  background-color: #000;
  opacity: .5;
}
.prompt{
  width: 500rpx;
  height: 340rpx;
  background-color: #fff;
  border-radius: 8rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  margin: -170rpx 0 0 -250rpx;
}
.protitle{
  width: 100%;
  text-align: center;
  font-weight: 500;
  margin: 20rpx 0rpx;
}
.proinner{
  width: 400rpx;
  margin: 0rpx 50rpx 14rpx;
  font-size: 28rpx;
}
.close{
  width: 200rpx;
  height: 64rpx;
  line-height: 64rpx;
  text-align: center;
  background-color: #577eff;
  color: #fff;
  border-radius: 12rpx;
  margin: auto;
  font-size: 32rpx;
}