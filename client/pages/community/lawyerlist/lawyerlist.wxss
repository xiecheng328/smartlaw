/* pages/community/lawyerlist/lawyerlist.wxss */
.lawyerlist{
  width: 100%;
  height: 220rpx;
  border-bottom: 1rpx solid #ddd;
}
.term{
  width: 100%;
  height: 220rpx;
}
.listtop{
  display: flex;
  flex-direction: row;
  width: 100%;
}
.headimg{
  width: 120rpx;
  height: 120rpx;
  margin: 14rpx;
  border-radius: 8rpx;
}
.listright{
  margin:14rpx 14rpx 14rpx 0rpx;
}
.starbox{
  display: flex;
  flex-direction: row;
  margin: 4rpx 0rpx;
}
.ostar{
  width: 20rpx;
  height: 20rpx;
  margin-right: 10rpx;
}
.fraction{
  font-size: 20rpx;
  color: #FFB400;
  line-height: 20rpx;
}
.lname{
  font-size: 30rpx;
}
.office{
  font-size: 20rpx;
}
.licensenum{
  font-size: 20rpx;
  color: #717171;
}
.listbottom{
  display: flex;
  flex-direction: row;
}
.worde{
  font-size: 28rpx;
  margin: 0rpx 14rpx; 
}
.fields{
  width: 120rpx;
  height: 40rpx;
  border: 1rpx solid #999999;
  border-radius: 4rpx;
  font-size: 24rpx;
  color: #999999;
  margin-right: 10rpx;
  text-align: center;
  line-height: 40rpx;
}