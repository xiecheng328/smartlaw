<!--pages/lawyer_assessment/lawyer_search/lawyer_details/lawyer_details.wxml-->
<view class='header'>
<image src='{{list.img}}'></image>
<view>{{list.msg}}</view>
</view>
<view class='content'>
  <view class='title'>{{messages1.resume}}</view>
  <view class='message'>   
    <view>胜率{{messages1.win}}</view>
    <view>{{messages1.num}}</view>    
  </view>
  <view class='title'>{{messages2.resume}}</view>
  <view class='message'>
  <view>整体胜率:{{messages2.win}}</view>
  <view>{{messages2.num}}</view>
  </view>
</view>
<view class='wrapper'>
  {{wrapper}}
</view>

<view class='lawyer-key-msg'>{{lawyerKey.msg}}</view>
<view class='lawyer-key-key'>{{lawyerKey.key}}</view>
