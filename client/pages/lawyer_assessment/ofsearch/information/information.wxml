<view class='doc-msg'>{{doc.msg}}</view>
<view class='doc-key'>{{doc.key}}</view>
<canvas canvas-id="canvas2" class='canvas'></canvas>

<view class='case-type-msg'>{{caseType.msg}}</view>
<view class='case-type-key'>{{caseType.key}}</view>
<canvas canvas-id="canvas4" class='canvas'></canvas>

<view class='case-msg'>{{case.msg}}</view>
<view class='case-key'>{{case.key}}</view>
